	<queryString>
		<![CDATA[SELECT mc.numero_factura, p.nombres, mc.fecha_emision,  mc.plazo_dias, (mc.fecha_emision + mc.plazo_dias) fecha_vencimiento, mc.monto_total, mc.saldo  
FROM movimiento_cabecera mc, proveedor p 
WHERE mc.proveedor_id = p.id 
AND mc.tipo_factura = 'CREDITO' 
AND mc.tipo_movimiento = 'COMPRA'
AND mc.saldo > 0
and mc.fecha_emision between  $P!{fecha_inicio} and  $P!{fecha_fin} ]]>
	</queryString>
