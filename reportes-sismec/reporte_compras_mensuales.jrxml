	<queryString>
		<![CDATA[select tabla.*, 
case when tabla.mes =1 then 'ENERO' 
 when  tabla.mes =2 then 'FEBRERO' 
 when  tabla.mes =3 then 'MARZO' 
 when  tabla.mes =4 then 'ABRIL' 
 when  tabla.mes =5 then 'MAYO' 
 when  tabla.mes =6 then 'JUNIO' 
 when tabla.mes =7 then 'JULIO' 
 when tabla.mes =8 then 'AGOSTO' 
 when tabla.mes =9 then 'SETIEMBRE' 
 when tabla.mes =10 then 'OCTUBRE' 
 when tabla.mes =11 then 'NOVIEMBRE' 
 when tabla.mes =12 then 'DICIEMBRE' end 
 from 
(
select 
sum(mc.monto_total) as monto_total,
extract ('month' from mc.fecha_emision) as mes
from movimiento_cabecera  mc
where  1=1
and mc.tipo_movimiento='COMPRA' 
--and mc.estado='COMPLETADO'
group by extract ('month' from mc.fecha_emision)
) as tabla
order by tabla.mes]]>
	</queryString>
