	<queryString>
		<![CDATA[select 
mc.fecha_emision,
mc.numero_factura,
c.nombres,
mc.tipo_factura,
mc.monto_total,
mc.estado,
mc.saldo,
mc.fecha_vencimiento

from  movimiento_cabecera mc 
left join cliente c on (c.id=mc.cliente_id)
where 1=1
and mc.estado!='COMPLETADO'
and tipo_factura='CREDITO'
and tipo_movimiento='VENTA'
	 $P!{parametros}]]>
	</queryString>
