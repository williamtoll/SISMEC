	<parameter name="fecha_inicio" class="java.lang.String"/>
	<parameter name="fecha_fin" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT sum(md.cantidad), p.descripcion
FROM movimiento_detalle md, producto p, movimiento_cabecera mc 
where md.producto_id = p.id
and mc.id = md.movimiento_cab_id
and mc.tipo_movimiento = 'VENTA'
and (p.tipo_producto_id = 1 or p.tipo_producto_id = 2) 
and mc.fecha_emision between $P!{fecha_inicio} and $P!{fecha_fin} 
group by p.descripcion
order by sum(md.cantidad) desc]]>
	</queryString>
